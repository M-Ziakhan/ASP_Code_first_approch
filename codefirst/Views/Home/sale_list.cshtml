@model IEnumerable<codefirst.Models.sale>

@{
    ViewBag.Title = "sale_list";
}

<h2>sale_list</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.sale_product)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sale_qty)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.sale_product)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sale_qty)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.sale_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.sale_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.sale_id })
        </td>
    </tr>
}

</table>
